USE Students;

-- Simple sum, max-min, avg
SELECT SUM(Age) AS "SumOfAges" FROM StudentInfo;
SELECT MIN(Age) AS "YoungestStudentAge" FROM StudentInfo;
SELECT MAX(Age) AS "EldestStudentName" FROM StudentInfo;
SELECT AVG(Age) AS "AverageAge" FROM StudentInfo;

-- Group by
SELECT AVG(Age) AS "AverageAge", Gender FROM StudentInfo GROUP BY Gender;